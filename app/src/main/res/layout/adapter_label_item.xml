<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="horizontal"
    android:paddingTop="@dimen/card_spacing"
    android:paddingBottom="@dimen/card_spacing">

    <TextView
        android:id="@+id/adapter_label_antenna_number_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:text="mAntennaNumber"
        android:textColor="@color/text_view_01" />

    <TextView
        android:id="@+id/adapter_label_rssi_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:text="mRSSI"
        android:textColor="@color/text_view_01" />

    <TextView
        android:id="@+id/adapter_label_time_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:text="mOperatingTime"
        android:textColor="@color/text_view_01" />

    <TextView
        android:id="@+id/adapter_label_epc_length_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:text="mEPCLength"
        android:textColor="@color/text_view_01" />

    <TextView
        android:id="@+id/adapter_label_epc_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:text="mEPC"
        android:textColor="@color/text_view_01" />

    <TextView
        android:id="@+id/adapter_label_tid_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center"
        android:text="mTID"
        android:textColor="@color/text_view_01" />

    <TextView
        android:id="@+id/adapter_label_inventory_number_tv"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:gravity="center"
        android:text="mInventoryNumber"
        android:textColor="@color/text_view_01" />


</LinearLayout>
